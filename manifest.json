{
  // Required
  "manifest_version": 2,
  "name": "The Pirate Bay for Dummies",
  "version": "1",
  "description": "Assists users finding quality torrents",
  "background": {
    "scripts": ["jquery-2.1.1.min.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "web_accessible_resources": [
  "style.css",
  "jquery-2.1.1.min.map",
  "images/Movie_Clip.png",
  "images/HD_Movie_Clip.png",
  "images/download-button.png",
  "images/tv.png",
  "images/ebook.png",
  "images/audio.png",
  "images/audio_book.png",
  "images/audio_book.png",
  "images/comics.png"
  ],
  "content_scripts": [
    {
      "matches": ["http://thepiratebay.se/*"],
      "css": ["style.css"],
      "js": ["jquery-2.1.1.min.js", "content.js"]
    }
  ],
  "permissions": [
    "http://thepiratebay.se/*",
    "tabs"
  ]
}  